{
  "name": "roomba-sdk",
  "version": "1.0.0",
  "description": "Unofficial iRobot Roomba 980 and wifi other enabled series library sdk - fork of dorita980",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint lib/ --ext .js,.jsx,.ts,.tsx",
    "test": "yarn run lint && mocha -r ts-node/register",
    "test:coverage:run": "istanbul cover _mocha -- $npm_package_config_mocha",
    "test:coverage:check": "istanbul check-coverage --functions 80",
    "test:coverage": "npm run test:coverage:run && npm run test:coverage:check",
    "getpassword": "node ./bin/getpassword.js",
    "build": "rm -rf ./dist && tsc"
  },
  "bin": {
    "rsdk-cli": "./dist/bin/getpassword.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hweeks/roomba-sdk.git"
  },
  "devDependencies": {
    "@types/node": "^12.12.30",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.0",
    "mocha": "^7.1.1",
    "proxyquire": "^1.7.10",
    "semistandard": "^9.1.0",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "unofficial",
    "iRobot",
    "Roomba",
    "980",
    "wifi",
    "i7",
    "e5",
    "960",
    "690",
    "675",
    "library",
    "sdk",
    "robot",
    "local",
    "cloud",
    "alexa",
    "vacuum",
    "cleaner",
    "IoT"
  ],
  "author": "koalazak <zak.tux@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koalazak/dorita980/issues"
  },
  "homepage": "https://github.com/koalazak/dorita980#readme",
  "dependencies": {
    "mqtt": "^2.15.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1"
  }
}
